{
  "name": "remirror-extension-flat-list",
  "type": "module",
  "version": "0.1.3",
  "description": "A Remirror extension based on prosemirror-flat-list",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/prosemirror-flat-list#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocavue/prosemirror-flat-list.git",
    "directory": "packages/remirror-extension"
  },
  "bugs": "https://github.com/ocavue/prosemirror-flat-list/issues",
  "keywords": [
    "ProseMirror",
    "Remirror",
    "list"
  ],
  "sideEffects": false,
  "main": "./dist/remirror-extension-flat-list.js",
  "module": "./dist/remirror-extension-flat-list.js",
  "types": "./dist/remirror-extension-flat-list.d.ts",
  "exports": {
    ".": {
      "types": "./dist/remirror-extension-flat-list.d.ts",
      "require": "./dist/remirror-extension-flat-list.cjs",
      "import": "./dist/remirror-extension-flat-list.js"
    },
    "./style.css": "./dist/style.css",
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "run-s build:tsup build:tsc build:api-extractor",
    "build:tsup": "tsup",
    "build:tsc": "tsc -b tsconfig.json",
    "build:api-extractor": "api-extractor run",
    "prepublishOnly": "nr build",
    "test": "vitest"
  },
  "dependencies": {
    "@remirror/core": "^2.0.12",
    "prosemirror-flat-list": "^0.3.4"
  },
  "peerDependencies": {
    "@remirror/pm": "^2.0.4"
  },
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "@microsoft/api-extractor": "^7.34.4",
    "@remirror/extension-blockquote": "^2.0.13",
    "@remirror/extension-hard-break": "^2.0.12",
    "@remirror/extension-heading": "^2.0.12",
    "@remirror/extension-link": "^2.0.14",
    "@remirror/pm": "^2.0.4",
    "@remirror/preset-core": "^2.0.15",
    "@remirror/react": "^2.0.27",
    "@remirror/styles": "^2.0.4",
    "@types/dedent": "^0.7.0",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitest/coverage-c8": "^0.29.2",
    "dedent": "^0.7.0",
    "execa": "^7.1.0",
    "jest-prosemirror": "^2.1.2",
    "jest-remirror": "^2.1.4",
    "jsdom": "^21.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-nesting": "^11.2.1",
    "prosemirror-model": "^1.19.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remirror": "^2.0.26",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "unified": "^10.1.2",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "typedocOptions": {
    "entryPoints": ["src/index.ts"]
  }
}
