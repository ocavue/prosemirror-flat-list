    "@microsoft/api-extractor": "^7.34.4",
    "@remirror/core": "^2.0.11",
    "@remirror/extension-blockquote": "^2.0.12",
    "@remirror/extension-heading": "^2.0.11",
    "@remirror/pm": "^2.0.3",
