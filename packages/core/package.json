{
  "name": "prosemirror-flat-list",
  "type": "module",
  "version": "0.5.6",
  "description": "Powerful list support for ProseMirror",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/prosemirror-flat-list#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocavue/prosemirror-flat-list.git",
    "directory": "packages/core"
  },
  "bugs": "https://github.com/ocavue/prosemirror-flat-list/issues",
  "keywords": [
    "ProseMirror",
    "list"
  ],
  "sideEffects": false,
  "main": "./dist/prosemirror-flat-list.js",
  "module": "./dist/prosemirror-flat-list.js",
  "types": "./dist/prosemirror-flat-list.d.ts",
  "exports": {
    ".": {
      "types": "./dist/prosemirror-flat-list.d.ts",
      "require": "./dist/prosemirror-flat-list.cjs",
      "import": "./dist/prosemirror-flat-list.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "pnpm run build:tsdown && pnpm run build:css",
    "build:tsdown": "tsdown",
    "build:css": "esbuild --target=chrome100 --bundle src/style.css --outfile=dist/style.css",
    "prepublishOnly": "pnpm run build",
    "test": "vitest"
  },
  "dependencies": {
    "prosemirror-commands": "^1.7.1",
    "prosemirror-inputrules": "^1.5.0",
    "prosemirror-model": "^1.25.3",
    "prosemirror-safari-ime-span": "^1.0.2",
    "prosemirror-state": "^1.4.3",
    "prosemirror-transform": "^1.10.4",
    "prosemirror-view": "^1.41.3"
  },
  "devDependencies": {
    "@remirror/core": "^3.0.2",
    "@remirror/extension-blockquote": "^3.0.2",
    "@remirror/extension-heading": "^3.0.2",
    "@remirror/extension-horizontal-rule": "^3.0.2",
    "@remirror/pm": "^3.0.1",
    "@remirror/preset-core": "^3.0.2",
    "@types/dedent": "^0.7.2",
    "@types/node": "^20.17.5",
    "@vitest/coverage-v8": "^3.2.4",
    "dedent": "^1.7.0",
    "esbuild": "^0.25.10",
    "jest-prosemirror": "^3.0.1",
    "jest-remirror": "^3.0.3",
    "jsdom": "^25.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remirror": "^3.0.3",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.2",
    "unified": "^11.0.5",
    "vite": "^6.3.6",
    "vitest": "^3.2.4"
  },
  "typedocOptions": {
    "entryPoints": [
      "src/index.ts"
    ]
  }
}
