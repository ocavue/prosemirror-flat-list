    "@vitest/coverage-c8": "^0.29.2",
    "dedent": "^0.7.0",
    "execa": "^7.0.0",
    "jest-prosemirror": "^2.1.1",
    "jest-remirror": "^2.1.3",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-nesting": "^11.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remirror": "^2.0.24",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "unified": "^10.1.2",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts"
  }
