{
  "name": "prosemirror-flat-list-monorepo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.1.0",
  "scripts": {
    "build": "nr -r --stream --sequential build",
    "dev": "nr -r --parallel dev",
    "test": "nr -r --stream test run",
    "api": "nr -r --stream build:tsc",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "change": "changeset",
    "ci:version": "changeset version && pnpm install --no-frozen-lockfile && nr build && nr api",
    "ci:publish": "pnpm publish -r --no-git-checks"
  },
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "@changesets/cli": "^2.26.1",
    "@ocavue/eslint-config": "^0.4.7",
    "eslint": "^8.36.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "typedoc": "^0.24.0-beta.7",
    "typescript": "^5.0.3"
  },
  "renovate": {
    "extends": [
      "github>ocavue/config-renovate"
    ],
    "dependencyDashboard": true
  }
}
