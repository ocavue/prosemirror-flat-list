{
  "name": "prosemirror-flat-list-monorepo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.29.0",
  "scripts": {
    "lint": "eslint .",
    "build": "nr -r --stream --sequential build",
    "dev": "nr -r --parallel dev",
    "api": "nr -r --stream build:tsc",
    "test": "nr -r --stream test run",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "ci:version": "changeset version && pnpm install --no-frozen-lockfile && nr build && nr api",
    "ci:publish": "pnpm publish -r --no-git-checks"
  },
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "typedoc": "^0.24.0-beta.2",
    "@changesets/cli": "^2.26.0",
    "@ocavue/eslint-config": "^0.4.7",
    "eslint": "^8.35.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "renovate": {
    "extends": [
      "github>ocavue/config-renovate"
    ]
  },
  "pnpm": {
    "patchedDependencies": {
      "typedoc-plugin-mdn-links@2.0.2": "patches/typedoc-plugin-mdn-links@2.0.2.patch",
      "nextra@2.2.18": "patches/nextra@2.2.18.patch"
    }
  }
}
